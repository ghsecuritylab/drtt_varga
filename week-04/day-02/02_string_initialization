#include <stdio.h>

int main() {
    char char_array[] = {'a', 'b', 'c', 'd','\0'};
    char char_array_the_same[] = "abcd";

	//TODO:
    // Print out the two arrays character-by-character
    printf("%s \n", char_array);

    for (int i = 0; i < 4; i ++) {

        printf("%c %c \n", char_array [i], char_array_the_same[i]);

    }
	//TODO:
    // What is the difference between the arrays?

    return 0;
}


#include <stdio.h>
#include <stdlib.h>

int main() {
    char char_array[] = {'a', 'b', 'c', 'd', '\0'};
    char char_array_the_same[] = "abcd";

	//TODO:
    // Print out the two arrays character-by-character
    printf("%s \n", char_array);
    printf("%s \n", char_array_the_same);
	//TODO:
    // What is the difference between the arrays?

    return 0;
}
