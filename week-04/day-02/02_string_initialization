#include <stdio.h>

int main()
{
    char char_array[] = {'a', 'b', 'c', 'd'};
    char char_array_the_same[] = "abcd";

    //TODO:
    // Print out the two arrays character-by-character

    int n = sizeof(char_array);

    for (int i = 0; i < n; i++) {

        printf("%c", char_array[i]);
    }

    printf("\n");

    int m = sizeof(char_array_the_same);

    for (int i = 0; i < m; i++) {

        printf("%c", char_array_the_same[i]);
    }

    //TODO:
    // What is the difference between the arrays? only the initialization

    return 0;
}
