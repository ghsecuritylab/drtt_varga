#include <stdio.h>

/* Write a non void function which has a paramters (let's say for example x).
The funtion than returns the "xth" fibonaci number.
What the hack is a fibonacci number? Search it on wikipedia!
*/

int fibonacci(int);

int main(){

    int xth_fib_num = fibonacci(6);
	printf("%d", xth_fib_num);


    return 0;
}

int fibonacci(int x){

    int fibo_x_minus_2 = 0;
    int fibo_x_minus_1 = 1;
    int fibo_x = 0;

    if(x < 1){
        return -1;
    }else if (x == 1) {
        return 0;
    }else if (x ==2){
        return 1;
    }else {
        for(int i = 0; i< (x-2); i++){

            fibo_x = fibo_x_minus_1 + fibo_x_minus_2;
            fibo_x_minus_2 = fibo_x_minus_1;
            fibo_x_minus_1 = fibo_x;
        }
        return fibo_x;
    }
}
