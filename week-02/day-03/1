#include <stdio.h>
#include <stdlib.h>
#include <stdio.h>



void TotoroLogoPrinter();
/* Write a void function which can print the following text on the screen:
------------
-- TOTORO --
------------
- GREENFOX -
------------
Name this function as "TotoroLogoPrinter" and call it from the main function!*/

void TotoroLogoPrinter(){
    printf("----------\n");
    printf("--TOTORO--\n");
    printf("-GREENFOX-\n");
    printf("----------\n");

    return ;

}

int main(){
      TotoroLogoPrinter();

    return 0;
}
